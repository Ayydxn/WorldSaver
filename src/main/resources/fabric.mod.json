{
	"schemaVersion": 1,
	"id": "world-saver",
	"version": "${version}",
	"name": "World Saver",
	"description": "A mod which creates backups of worlds and stores them in a Google Drive folder.",
	"authors": [
		"Ayydxn"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "LGPL-3.0-or-later",
	"icon": "assets/world-saver/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.ayydxn.worldsaver.WorldSaverCommonMod"
		],
		"client": [
			"me.ayydxn.worldsaver.WorldSaverClientMod"
		],
		"server": [
			"me.ayydxn.worldsaver.WorldSaverServerMod"
		]
	},
	"mixins": [
		"world-saver.mixins.json"
	],
	"depends": {
		"minecraft": "~${minecraft_version}",
		"fabricloader": ">=${fabric_loader_version}",
		"fabric-api": "${fabric_api_version}"
	}
}